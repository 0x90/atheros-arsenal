config regmon 'regmon'
    list 'regmonpath' '/sys/kernel/debug/ieee80211/phy0/regmon'
    option 'samplingrate' '500000000'
    option 'timefrom' '0'
    option 'usesystime' '0'
    option 'absolutecounter' '3'
    option 'busycounter' 'busy_count'
    option 'startindex' '4'
    list 'metrics' 'tx_count'
    list 'metrics' 'rx_count'
    list 'metrics' 'busy_count'

config collectd 'collectd'
    option 'interval' '5'
    option 'enablelog' '0'

config rrdtool 'rrdtool'
    option 'imagepath' '/tmp/regmon'
    option 'width' '800'
    option 'height' '500'
    option 'stacked' '1'
    option 'shape' 'AREA'
    option 'highlight' '1'
